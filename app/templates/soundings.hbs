<div class="page-header">
	<h1>Soundings</h1>
</div>
<ul>
	{{#each sounding in model}}
		<li>
			{{#link-to 'sounding' sounding.id}}
				{{sounding.forecast}} - ({{sounding.loc.latitude}},{{sounding.loc.longitude}})
			{{/link-to}}
		</li>
	{{/each}}
</ul>
{{pager-component pageCount=pageCount currentPage=page action="changePage"}}
