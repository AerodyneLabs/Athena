<h2>Parachute</h2>
<div class="row">
  <div class="col-md-4">
    <form class="form-horizontal">
      <div class="form-group">
        <label for="parachuteSelect" class="col-md-4 control-label">Parachute</label>
        <div class="col-md-8">
          {{view "select" elementId="parachuteSelect" content=parachutes value=parachute class="form-control"}}
        </div>
      </div>
      <div class="form-group">
        <label for="parachuteArea" class="col-md-4 control-label">Area</label>
        <div class="col-md-8">
          <div class="input-group">
            {{input elementId="parachuteArea" value=parachuteArea class="form-control" readonly=isStatic}}
            <span class="input-group-addon">units</span>
          </div>
        </div>
      </div>
      <div class="form-group">
        <label for="parachuteDrag" class="col-md-4 control-label">Drag Coef</label>
        <div class="col-md-8">
          {{input elementId="parachuteDrag" value=parachuteDrag class="form-control" readonly=isStatic}}
        </div>
      </div>
      {{#if isCustom}}
        blah blah blah
      {{/if}}
      <div class="btn-group pull-right" role="group">
        <button class="btn btn-default" type="button" {{action "back"}}>Back</button>
        <button class="btn btn-primary" type="button" {{action "next"}}>Next</button>
      </div>
    </form>
  </div>
  <div class="col-md-8">
    Graph
  </div>
</div>
